{
    "name": "atoship/php-sdk",
    "description": "Official PHP SDK for the atoship API",
    "keywords": [
        "atoship",
        "shipping",
        "logistics",
        "api",
        "sdk",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "atoship Team",
            "email": "support@atoship.com",
            "homepage": "https://atoship.com"
        }
    ],
    "homepage": "https://atoship.com/docs",
    "support": {
        "email": "support@atoship.com",
        "issues": "https://github.com/atoship-LLC/atoship-php/issues",
        "source": "https://github.com/atoship-LLC/atoship-php",
        "docs": "https://atoship.com/docs"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^1.1|^2.0|^3.0",
        "symfony/validator": "^5.0|^6.0|^7.0",
        "symfony/property-access": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4",
        "guzzlehttp/psr7": "^2.0",
        "symfony/dotenv": "^5.0|^6.0|^7.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "atoship\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "atoship\\SDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse src tests --level=8",
        "cs-fix": "php-cs-fixer fix --config=.php-cs-fixer.php",
        "cs-check": "php-cs-fixer fix --config=.php-cs-fixer.php --dry-run --diff",
        "quality": [
            "@cs-check",
            "@phpstan",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}